<h1>All Your Vehicles</h1>
<%= link_to "Add a Vehicle", new_driver_vehicle_path(@driver) %>
<br>
<br>
<% @driver.vehicles.each do |vehicle| %>
    <li><%= vehicle.year %> <%= vehicle.make %> <%= vehicle.model%> <%= link_to "View", vehicle_path(vehicle) %></li><li><%= link_to "Remove", driver_vehicle_path(@driver, vehicle), method: 'delete' %></li>
    <br><br><br><br>
<% end %>